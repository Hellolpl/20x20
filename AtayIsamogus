local LocalPlayer = game:GetService("Players").LocalPlayer
local RootPart = LocalPlayer.Character.HumanoidRootPart
local cframe = RootPart.CFrame

local Remote = game:GetService("ReplicatedStorage").Events.PlaceStructure

Remote:FireServer("Chest",
    cframe * CFrame.new(12, 24, 30),
0)
Remote:FireServer("Chest",
    cframe * CFrame.new(12, 18, 30),
0)
Remote:FireServer("Chest",
    cframe * CFrame.new(12, 12, 30),
0)
Remote:FireServer("Chest",
    cframe * CFrame.new(12, 6, 30),
0)
Remote:FireServer("Chest",
    cframe * CFrame.new(12, -0, 30),
0)
Remote:FireServer("Chest",
    cframe * CFrame.new(12, -6, 30),
0)



